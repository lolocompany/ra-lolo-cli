import { ListProps, FilterProps } from 'react-admin'

const {{resourceName}}Filter: React.FC<FilterProps>  = (props) => (
  <Filter {...props}>
    {{#each components.filterView}}
      {{{this}}}
    {{/each}}
  </Filter>
);

const {{resourceName}}List: React.FC<ListProps> = (props) => (
<List filters={<{{resourceName}}Filter />} {...props}>
  <Datagrid>
    {{#each components.listView}}
      {{{this}}}
    {{/each}}
  </Datagrid>
</List>
);

export default {{resourceName}}List;