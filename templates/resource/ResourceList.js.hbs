import React from 'react';

const {{resourceName}}Filter = (props) => (
  <Filter {...props}>
    {{#each components.filterView}}
      {{{this}}}
    {{/each}}
  </Filter>
);

const {{resourceName}}List = (props) => (
<List filters={<{{resourceName}}Filter />} {...props}>
  <Datagrid>
    {{#each components.listView}}
      {{{this}}}
    {{/each}}
  </Datagrid>
</List>
);

export default {{resourceName}}List;